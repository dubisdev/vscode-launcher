{
  "name": "vscode-app-launcher",
  "displayName": "App Launcher",
  "description": "Launch your system apps from VS Code",
  "version": "0.0.0",
  "publisher": "dubisdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/dubisdev/vscode-app-launcher.git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.launchApp",
    "onCommand:extension.launchDefaultApp"
  ],
  "main": "./out/extension.js",
  "icon": "not-found.png",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "extension.launchApp",
        "title": "Launch App"
      },
      {
        "command": "extension.launchDefaultApp",
        "title": "Todoist Open [Project]"
      }
    ],
    "keybindings": [
      {
        "command": "extension.launchApp",
        "key": "alt+l l"
      },
      {
        "command": "extension.launchDefaultApp",
        "key": "alt+l d"
      }
    ],
    "configuration": [
      {
        "title": "App Launcher",
        "properties": {
          "launcher.defaultApp": {
            "type": "string",
            "default": null,
            "description": "The default app to launch when no app is specified in the command",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "1.66.0",
    "electron": "^18.2.0",
    "fetch-installed-software": "^0.0.7",
    "glob": "^7.1.6",
    "installed-sw": "1.0.1",
    "lodash": "^4.17.21",
    "mocha": "^7.1.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  }
}
